{
	"name": "alexmayhew-dev",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev --turbopack",
		"prebuild": "npm run clean:cache",
		"build": "NODE_ENV=production next build",
		"build:clean": "npm run clean:full && NODE_ENV=production next build",
		"clean:cache": "rm -rf .next node_modules/.cache",
		"clean:full": "rm -rf .next node_modules/.cache && npm cache clean --force",
		"postinstall": "rm -rf .next/cache 2>/dev/null; fumadocs-mdx",
		"start": "next start",
		"lint": "eslint . --ext .ts,.tsx,.js,.jsx",
		"lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"typecheck": "tsc --noEmit",
		"test": "vitest",
		"test:run": "vitest run",
		"test:coverage": "vitest run --coverage",
		"test:e2e": "playwright test",
		"test:e2e:ui": "playwright test --ui",
		"test:e2e:headed": "playwright test --headed",
		"validate": "npm run typecheck && npm run lint && npm run test:run",
		"deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
		"upload": "opennextjs-cloudflare build && opennextjs-cloudflare upload",
		"preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
		"cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts",
		"prepare": "husky"
	},
	"dependencies": {
		"@keystatic/core": "^0.5.48",
		"@keystatic/next": "^5.0.4",
		"@marsidev/react-turnstile": "^1.4.1",
		"@opennextjs/cloudflare": "^1.14.4",
		"@react-email/components": "^1.0.4",
		"@sentry/nextjs": "^10.34.0",
		"@types/mdx": "^2.0.13",
		"clsx": "^2.1.1",
		"framer-motion": "^12.26.2",
		"fumadocs-core": "^15.8.5",
		"fumadocs-mdx": "^14.2.5",
		"fumadocs-ui": "^15.8.5",
		"lenis": "^1.3.17",
		"lucide-react": "^0.562.0",
		"next": "15.5.9",
		"react": "19.1.4",
		"react-dom": "19.1.4",
		"resend": "^6.7.0",
		"tailwind-merge": "^3.4.0",
		"zod": "^4.3.5"
	},
	"devDependencies": {
		"@axe-core/playwright": "^4.11.0",
		"@cloudflare/workers-types": "^4.20260117.0",
		"@eslint/eslintrc": "^3",
		"@lhci/cli": "^0.15.1",
		"@playwright/test": "^1.57.0",
		"@tailwindcss/postcss": "^4",
		"@testing-library/dom": "^10.4.1",
		"@testing-library/react": "^16.3.1",
		"@types/node": "^20.19.29",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"@types/testing-library__jest-dom": "^5.14.9",
		"@vitejs/plugin-react": "^5.1.2",
		"@vitest/coverage-v8": "^4.0.17",
		"critters": "^0.0.23",
		"eslint": "^9",
		"eslint-config-next": "15.4.6",
		"husky": "^9.1.7",
		"jsdom": "^27.4.0",
		"lint-staged": "^16.2.7",
		"prettier": "^3.7.4",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"tailwindcss": "^4",
		"typescript": "^5",
		"vitest": "^4.0.17",
		"wrangler": "^4.59.1"
	},
	"lint-staged": {
		"*.{ts,tsx}": [
			"eslint --fix",
			"prettier --write"
		],
		"*.{js,jsx,mjs,cjs}": [
			"eslint --fix",
			"prettier --write"
		],
		"*.{json,md,css}": [
			"prettier --write"
		]
	}
}
